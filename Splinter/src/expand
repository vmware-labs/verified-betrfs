                #[verifier::inline]
                #[verus::internal(open)]
                #[verus::internal(verus_macro)]
                pub fn get_initialize_0(self) -> LikesJournal::State {
                    ::builtin::get_variant_field(self, "initialize", "0")
                }
                #[cfg(verus_keep_ghost)]
                #[allow(non_snake_case)]
                #[verus::internal(spec)]
