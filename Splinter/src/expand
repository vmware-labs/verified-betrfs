                { { ::std::rt::begin_panic("explicit panic") } }
            }
            #[verus::internal(proof)]
            pub fn sub_disk_ranking(self, big: DiskView) {
                { { ::std::rt::begin_panic("explicit panic") } }
            }
            #[verus::internal(proof)]
            pub fn sub_disk_interp(self, big: DiskView, ptr: Pointer) {
