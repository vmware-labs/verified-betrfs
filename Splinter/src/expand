                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                pub fn get_query_0(self) -> QueryReceipt {
                    ::builtin::get_variant_field(self, "query", "0")
                }
