            #[verus::internal(verus_macro)]
            pub enum Label {
                OperateOp {
                    base_op: AsyncMap::Label,
                },
                CrashOp,
                SyncOp,
                ReqSyncOp {
                    sync_req_id: SyncReqId,
                },
                ReplySyncOp {
                    sync_req_id: SyncReqId,
                },
                Noop,
            }
            #[verus::internal(verus_macro)]
            #[cfg(verus_keep_ghost)]
            #[automatically_derived]
