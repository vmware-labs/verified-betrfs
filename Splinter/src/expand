                #[cfg(verus_keep_ghost)]
                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                pub fn is_QueryEndLsn(&self) -> bool {
                    ::builtin::is_variant(self, "QueryEndLsn")
                }
                #[cfg(verus_keep_ghost)]
                #[allow(non_snake_case)]
                #[verus::internal(spec)]
