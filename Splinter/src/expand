                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                pub fn is_put(&self) -> bool {
                    ::builtin::is_variant(self, "put")
                }
                #[cfg(verus_keep_ghost)]
                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
