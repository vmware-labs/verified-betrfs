            #[verus::internal(verus_macro)]
            pub enum Label {
                ReadForRecovery {
                    messages: MsgHistory,
                },
                FreezeForCommit {
                    frozen_journal: TruncatedJournal,
                },
                QueryEndLsn {
                    end_lsn: LSN,
                },
                Put {
                    messages: MsgHistory,
                },
                DiscardOld {
                    start_lsn: LSN,
                    require_end: LSN,
                },
                Internal {},
            }
            #[verus::internal(verus_macro)]
            #[cfg(verus_keep_ghost)]
