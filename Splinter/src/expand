                    }
                    __VERUS_REVEAL_INTERNAL__
                }, 1);
            ::builtin::reveal_hide_({
                    #[verus::internal(reveal_fn)]
                    fn __VERUS_REVEAL_INTERNAL__() {
                        ::builtin::reveal_hide_internal_path_(AbstractJournal::State::next_by)
                    }
                    __VERUS_REVEAL_INTERNAL__
                }, 1);
            ::builtin::reveal_hide_({
                    #[verus::internal(reveal_fn)]
