                #[verifier::inline]
                #[verus::internal(open)]
                #[verus::internal(verus_macro)]
                pub fn is_internal_mini_allocator_fill(&self) -> bool {
                    ::builtin::is_variant(self, "internal_mini_allocator_fill")
                }
                #[cfg(verus_keep_ghost)]
