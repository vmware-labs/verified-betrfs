            #[verus::internal(verus_macro)]
            pub enum Label {
                ReadForRecovery {
                    messages: MsgHistory,
                },
                FreezeForCommit {
                    frozen_journal: TruncatedJournal,
                },
                QueryEndLsn {
                    end_lsn: LSN,
                },
                Put {
                    messages: MsgHistory,
                },
                DiscardOld {
                    start_lsn: LSN,
                    require_end: LSN,
                },
                Internal {},
            }
            #[verus::internal(verus_macro)]
            #[cfg(verus_keep_ghost)]
            #[automatically_derived]
            impl Label {
                #[cfg(verus_keep_ghost)]
                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                #[verus::internal(verus_macro)]
                pub fn is_ReadForRecovery(&self) -> bool {
                    ::builtin::is_variant(self, "ReadForRecovery")
                }
