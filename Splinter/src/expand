            #[verus::internal(verus_macro)]
            pub enum Label {
                LoadEphemeralFromPersistentLabel {
                    end_lsn: LSN,
                },
                PutRecordsLabel {
                    records: MsgHistory,
                },
                QueryLabel {
                    end_lsn: LSN,
                    key: Key,
                    value: Value,
                },
                InternalLabel,
                CommitStartLabel {
                    new_boundary_lsn: LSN,
                },
                CommitCompleteLabel,
                CrashLabel,
            }
