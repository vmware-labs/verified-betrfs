                                                            (#[verifier::custom_err("cannot prove this condition holds")] (!pre_ephemeral.sync_reqs.dom().contains(sync_req_id))
                                                                    &&
                                                                    (#[verifier::custom_err("cannot prove this condition holds")] (CrashTolerantJournal::State::next(pre.journal,
