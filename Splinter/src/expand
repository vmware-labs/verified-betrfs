                put(),
                freeze_as(),
                internal_flush_memtable(),
                internal_grow(),
                internal_split(Path, SplitRequest),
                internal_flush(Path, nat, nat),
                internal_compact(Path, nat, nat, Buffer),
                internal_noop(),
                dummy_to_use_type_params(State),
            }
