            #[verus::internal(verus_macro)]
            pub enum Label {
                Query {
                    end_lsn: LSN,
                    key: Key,
                    value: Value,
                },
                Put {
                    puts: MsgHistory,
                },
                FreezeAs {
                    stamped_betree: StampedBetree,
                },
                Internal {},
            }
            #[verus::internal(verus_macro)]
            #[cfg(verus_keep_ghost)]
            #[automatically_derived]
            impl Label {
                #[cfg(verus_keep_ghost)]
