            #[verus::internal(verus_macro)]
            pub enum Label {
                Query {
                    end_lsn: LSN,
                    key: Key,
                    value: Value,
                },
                Put {
                    puts: MsgHistory,
                },
                FreezeAs {
                    stamped_betree: StampedBetree,
                },
                Internal {},
            }
