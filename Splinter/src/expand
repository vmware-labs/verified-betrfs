                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                #[verus::internal(verus_macro)]
                pub fn get_ReadForRecovery_messages(self) -> MsgHistory {
                    ::builtin::get_variant_field(self, "ReadForRecovery",
                        "messages")
                }
                #[cfg(verus_keep_ghost)]
                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                #[verus::internal(verus_macro)]
