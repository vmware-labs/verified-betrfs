                }
                #[cfg(verus_keep_ghost_body)]
                #[verus::internal(verus_macro)]
                #[verifier::spec]
                #[verus::internal(open)]
                pub fn freeze_as_enabled(pre: Self, lbl: Label)
                    -> ::core::primitive::bool {
