                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                #[verus::internal(verus_macro)]
                pub fn is_Put(&self) -> bool {
                    ::builtin::is_variant(self, "Put")
                }
                #[cfg(verus_keep_ghost)]
                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                #[verus::internal(verus_macro)]
                pub fn get_Put_records(self) -> MsgHistory {
