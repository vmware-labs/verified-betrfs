                #[cfg(verus_keep_ghost_body)]
                #[verus::internal(verus_macro)]
                #[verifier::spec]
                #[verus::internal(open)]
                pub fn internal_noop_enabled(pre: Self, lbl: Label)
                    -> ::core::primitive::bool {
                    {
                        let tmp_for_match_0 = lbl;
                        (#[verifier::custom_err("cannot prove this condition holds")] (match tmp_for_match_0
