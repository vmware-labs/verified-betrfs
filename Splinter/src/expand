                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                pub fn get_RequestOp_req(self) -> Request {
                    ::builtin::get_variant_field(self, "RequestOp", "req")
                }
                #[cfg(verus_keep_ghost)]
                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                pub fn is_ExecuteOp(&self) -> bool {
                    ::builtin::is_variant(self, "ExecuteOp")
