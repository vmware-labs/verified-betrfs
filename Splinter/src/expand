            #[verus::internal(spec(checked))]
            pub fn _dr(self, lsn: LSN) -> MsgHistory {
                { { ::std::rt::begin_panic("explicit panic") } }
            }
            #[doc =
            " Returns a StampedMap formed by applying the operations in self (MsgHistory)"]
            #[doc = " to the StampedMap \"orig\"."]
