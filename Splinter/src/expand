            #[verus::internal(verus_macro)]
            pub enum Label {
                LoadEphemeralFromPersistentLabel,
                ReadForRecoveryLabel {
                    records: MsgHistory,
                },
                QueryEndLsnLabel {
                    end_lsn: LSN,
                },
                PutLabel {
                    records: MsgHistory,
                },
                InternalLabel,
                QueryLsnPersistenceLabel {
                    sync_lsn: LSN,
                },
                CommitStartLabel {
                    new_boundary_lsn: LSN,
                    max_lsn: LSN,
                },
                CommitCompleteLabel {
                    require_end: LSN,
                },
