                                        match tmp_for_match_0 {
                                            Label::Query { end_lsn, key, value } =>
                                                (end_lsn, key, value),
                                            _ => ::vstd::pervasive::arbitrary(),
                                        };
                                    (#[verifier::custom_err("cannot prove this condition holds")] (::builtin::spec_eq(end_lsn,
                                                    pre.memtable.seq_end)) &&
