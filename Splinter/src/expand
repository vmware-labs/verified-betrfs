        #[verifier::ext_equal]
        #[verus::internal(verus_macro)]
        pub enum BetreeNode {
            Nil,
            Node {
                buffer: Buffer,
                pivots: PivotTable,
                children: Seq<BetreeNode>,
            },
        }
        #[verus::internal(verus_macro)]
        #[cfg(verus_keep_ghost)]
        #[automatically_derived]
        impl BetreeNode {
