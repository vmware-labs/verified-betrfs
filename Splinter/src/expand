            pub enum Step {
                read_for_recovery(),
                freeze_for_commit(),
                query_end_lsn(),
                put(),
                discard_old(),
                internal_journal_marshal(LSN, Address,
                    LinkedJournal_v::LinkedJournal::State),
                internal_no_op(),
                dummy_to_use_type_params(State),
            }
