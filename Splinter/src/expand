                }
                #[verus::internal(open)]
                #[verus::internal(spec(checked))]
                pub fn imperative_matches_transitive(self) -> bool {
                    { { ::std::rt::begin_panic("explicit panic") } }
                }
                #[verus::internal(proof)]
                pub fn discard_old_step_preserves_wf_disk_view(pre: Self,
                    post: Self, lbl: Label) {
                    { { ::std::rt::begin_panic("explicit panic") } }
                }
                #[verus::internal(proof)]
                pub fn discard_old_step_preserves_wf_and_index(pre: Self,
                    post: Self, lbl: Label) {
                    { { ::std::rt::begin_panic("explicit panic") } }
                }
                #[verus::internal(proof)]
                pub fn discard_old_maintains_repr_index(pre: Self, post: Self,
                    lbl: Label) {
                    { { ::std::rt::begin_panic("explicit panic") } }
                }
            }
        }
    }
