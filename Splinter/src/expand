                #[allow(non_snake_case)]
                #[verus::internal(spec)]
                #[verifier::inline]
                #[verus::internal(open)]
                #[verus::internal(verus_macro)]
                pub fn get_Query_end_lsn(self) -> LSN {
                    ::builtin::get_variant_field(self, "Query", "end_lsn")
